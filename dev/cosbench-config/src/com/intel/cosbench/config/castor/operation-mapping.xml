<?xml version="1.0" encoding="UTF-8"?>

<mapping>

	<class name="com.intel.cosbench.config.Operation">

		<mapt-to xml="operation" />

		<field name="type" type="string">
			<bind-xml name="type" node="attribute" />
		</field>

		<field name="ratio" type="integer">
			<bind-xml name="ratio" node="attribute" />
		</field>

		<field name="division" type="string">
			<bind-xml name="division" node="attribute" />
		</field>

		<field name="config" type="string">
			<bind-xml name="config" node="attribute" />
		</field>
		
		<field name="id" type="string">
			<bind-xml name="id" node="attribute" />
		</field>

	</class>

</mapping>
